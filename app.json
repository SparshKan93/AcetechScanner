{
  "expo": {
    "name": "acetechnoid_scanner",
    "slug": "acetechnoid_scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1024Ace.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/bmi.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/512Ace.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sparshkan93.acetechnoid_scanner",
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": ["expo-font", "expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "613c5f66-1925-422f-be30-6d05438272ad"
      }
    }
  }
}
